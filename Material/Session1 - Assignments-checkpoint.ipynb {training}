     "output_type": "stream",
     "text": [
      "The number is zero.\n"
     ]
    }
   ],
   "source": [
    "num = float(input('Pls, Enter the  number'))\n",
    "if num > 0:\n",
    "     print(\"The number is positive.\")\n",
    "    \n",
    "elif num < 0:\n",
    "    print(\"The number is negative.\")\n",
    "    \n",
    "else:\n",
    "    print(\"The number is zero.\")"
   ]
